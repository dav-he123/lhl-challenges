<title>Create New TODO list</title>

<body>
  <%- include('partials/_header') %>

  <h3>Create TODO List</h3>

  <form class="form-inline" action="/todo" method="POST">
    <div class="form-group mb-2">
      <label for="name">Enter a TODO List:</label>
      <input
        class="form-control"
        type="text"
        name="name"
        placeholder="ToDo Name"
        style="width: 300px; margin: 1em;"
      />
      <button type="submit" class="btn btn-primary">Add</button>
    </div>
  </form>
</body>
